@font-face {
	font-family: 'Appetite';
	src: url('/Praktika/Laba/fonts/appetite.eot'); /* IE 9 Compatibility Mode */
	src: url('/Praktika/Laba/fonts/appetite.eot?#iefix') format('embedded-opentype'), /* IE < 9 */
		url('/Praktika/Laba/fonts/appetite.woff2') format('woff2'), /* Super Modern Browsers */
		url('/Praktika/Laba/fonts/appetite.woff') format('woff'), /* Firefox >= 3.6, any other modern browser */
		url('/Praktika/Laba/fonts/appetite.ttf') format('truetype'), /* Safari, Android, iOS */
		url('/Praktika/Laba/fonts/appetite.svg#appetite') format('svg'); /* Chrome < 4, Legacy iOS */
}